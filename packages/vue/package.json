{
  "name": "@flex-url/vue",
  "version": "0.1.2",
  "description": "Vue components for flex-url",
  "homepage": "https://flex-url.opensoutheners.com",
  "repository": {
    "type": "git",
    "url": "git+https://d8vjork@github.com/open-southeners/flex-url.git",
    "directory": "packages/vue"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bundle": "microbundle -f es,cjs,umd --globals @inertiajs/inertia",
    "watch": "yarn run bundle watch",
    "clean": "rimraf dist",
    "build": "npm-run-all --silent clean -p bundle",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "flex-url": "workspace:^",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "@inertiajs/inertia": "^0.11"
  },
  "devDependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "microbundle": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "xo": "^0.53.0"
  }
}
