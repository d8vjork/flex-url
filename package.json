{
  "name": "flex-url",
  "description": "Zero-dependency utility for parsing and modifying URL params",
  "version": "0.8.0",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://flex-url.opensoutheners.com",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "c8": "^7.12.0",
    "chai": "^4.3.4",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "microbundle": "^0.14.2",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bundle": "microbundle -f es,cjs,umd",
    "watch": "yarn run bundle watch",
    "clean": "rimraf dist",
    "build": "npm-run-all --silent clean -p bundle",
    "prepublishOnly": "yarn run build",
    "test": "c8 mocha"
  },
  "files": [
    "dist/*"
  ]
}
