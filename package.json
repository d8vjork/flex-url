{
  "name": "flex-url",
  "description": "Almost zero-dependency utility for parsing and modifying URL params",
  "version": "0.1.0",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.mjs",
  "umd:main": "dist/index.umd.js",
  "source": "src/index.ts",
  "typings": "types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/url-parse": "^1.4.5",
    "chai": "^4.3.4",
    "esm": "^3.2.25",
    "microbundle": "^0.14.2",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "qss": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "url-parse": "^1.5.3"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bundle": "microbundle -f es,cjs,umd",
    "watch": "microbundle --watch",
    "clean": "rimraf dist",
    "build": "npm-run-all --silent clean -p bundle",
    "prepublish": "yarn run build",
    "test": "mocha test"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "esm"
    ],
    "spec": [
      "tests/*_test.ts"
    ]
  },
  "files": [
    "dist/*"
  ]
}
